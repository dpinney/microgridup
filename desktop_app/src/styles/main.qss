QPushButton {
    background-color: rgba(255, 255, 255, 1);
    border: 0;
    border-radius: 10px;
    min-height: 48px;
    max-height: 48px;
    min-width: 48px;
    max-width: 48px;
}

QPushButton:hover {
    background-color: rgba(255, 255, 255, .2);
}

QPushButton:pressed {
    background-color: rgba(255, 255, 255, .4);
}
